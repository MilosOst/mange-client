{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "linebreak-style": ["error", "unix"],
        "no-console": 1,
        "no-multiple-empty-lines": ["error", { "max": 1 }],
        "react/prop-types": 0,
        "object-curly-spacing": ["warn", "always"]
    }
}
